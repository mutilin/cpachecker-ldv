<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="600" memlimit="2000">
  <test>
    <sourcefiles>
      <includesfile>test/test-sets/symbpredabsCPA.set</includesfile>
    </sourcefiles>
    <options>
      <option name="-setprop">output.disable=true</option>
      <option name="-config">test/config/symbpredabsCPA-lbe.properties</option>
      <option name="-heap">1200m</option>
    </options>
  </test>
  <columns>
    <column title="total">time for CPAchecker</column>
    <column title="reached">reached set</column>
    <column title="abstractions">Number of abstractions</column>
    <column title="refinements">Number of refinements</column>
  </columns>
</benchmark>
