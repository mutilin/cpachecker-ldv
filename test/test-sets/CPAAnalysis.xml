<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="300">
<option name="-heap">3000M</option>
<option name="-noout"/>
<option name="-disable-java-assertions"/>
<option name="-setprop">statistics.memory=false</option>

<rundefinition name="ReachDef_ExplicitDF">
	<option name="-reachingdef-explicit"/>
	<option name="-setprop">cpa.explicit.merge=JOIN</option>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="ReachDef_ExplicitJOINSEP">
	<option name="-reachingdef-explicit"/>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="ReachDef_ExplicitSEPJOIN">
	<option name="-reachingdef-explicit"/>
	<option name="-setprop">cpa.explicit.merge=JOIN</option>
	<option name="-setprop">cpa.reachdef.merge=SEP</option>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="ReachDef_ExplicitMC">
	<option name="-reachingdef-explicit"/>
	<option name="-setprop">cpa.reachdef.merge=SEP</option>
		<sourcefiles name="DFS">
			<option name="-setprop">analysis.traversal.order=DFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="ReachDefDF">
	<option name="-reachingdefinition"/>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="ExplicitDF">
	<option name="-explicit-trac-all"/>
	<option name="-setprop">cpa.explicit.merge=JOIN</option>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_6_safe.c</include>
			<include>test/programs/benchmarks/locks/test_locks_7_safe.c</include>
			<include>test/programs/benchmarks/bitvector/s3*_safe.BV.c.cil.c</include>
		</sourcefiles>
</rundefinition>

<rundefinition name="StructureTest">
	<option name="-reachingdef-explicit"/>
	<option name="-setprop">cpa.explicit.merge=JOIN</option>
		<sourcefiles name="BFS">
			<option name="-setprop">analysis.traversal.order=BFS</option>
			<include>test/programs/benchmarks/locks/test_locks_5_safe.c</include>
			<include>test/programs/nestedLocks/test_locks_while_seq_5_safe.c</include>
			<include>test/programs/nestedLocks/test_locks_while_mix_5_safe.c</include>
			<include>test/programs/nestedLocks/test_locks_while_nest_5_safe.c</include>
		</sourcefiles>
</rundefinition>

<columns>
<column title="Total time">Total time for CPAchecker</column>
<column title="Set up time">Time for analysis setup</column>
<column title="Analysis time">Time for Analysis</column>
<column title="Precision time">Time for precision adjustment</column>
<column title="Transfer time">Time for transfer relation</column>
<column title="Merge time">Time for merge operator</column>
<column title="Stop time">Time for stop operator</column>
<column title="#Removed Elems">Number of times merged</column>
</columns>
</benchmark>