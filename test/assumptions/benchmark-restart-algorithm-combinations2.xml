<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="900" memlimit="14000">
  <test name="combinations-d">
    <sourcefiles>
      <includesfile>test/assumptions/assumptions.set</includesfile>
    </sourcefiles>
    <option name="-config">test/config/restartAlgorithm-comb-d.properties</option>
    <option name="-setprop">output.disable=true</option>
    <option name="-heap">4000m</option>
  </test>
  <test name="combinations-e">
    <sourcefiles>
      <includesfile>test/assumptions/assumptions.set</includesfile>
    </sourcefiles>
    <option name="-config">test/config/restartAlgorithm-comb-e.properties</option>
    <option name="-setprop">output.disable=true</option>
    <option name="-heap">4000m</option>
  </test>
  <columns>
    <column title="total">time for CPAchecker</column>
  </columns>
</benchmark>