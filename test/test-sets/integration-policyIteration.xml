<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="60" hardtimelimit="90" memlimit="3000" cpuCores="2">

  <option name="-noout"/>
  <option name="-stator"/>

  <rundefinition>
    <!-- Integer Tests -->
    <sourcefiles>
        <include>../programs/stator/*.c</include>
    </sourcefiles>
</rundefinition>

<columns>
    <column title="total">time for policy iteration</column>
    <column title="reached">reached set</column>
</columns>
</benchmark>
