<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="3600" memlimit="2000">
  <test name="restart-test">
    <sourcefiles>
      <include>test/programs/nondet_init_programs/ntdrivers-simplified/*.cil.c</include>
      <include>test/programs/nondet_init_programs/ssh-simplified/*.cil.c</include>
      <include>test/programs/nondet_init_programs/systemc/*.cil.c</include>
    </sourcefiles>
    <options>
      <option name="-config">restartAlgorithm.properties</option>
      <option name="-setprop">output.disable=true</option>
    </options>
  </test>
  <columns>
    <column title="total">time for CPAchecker</column>
  </columns>
</benchmark>