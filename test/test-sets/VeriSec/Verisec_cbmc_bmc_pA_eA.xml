<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="10" memlimit="2000">
  <option name="-noout"/>
  <option name="-heap">1200m</option>

  <test name="bmc">
    <sourcefiles>
      <includesfile>test/test-sets/VeriSec/Verisec_cbmc_bmc_pA_eA-set.set</includesfile>
    </sourcefiles>
    <option name="-bmc"/>
  </test>
  
  <test name="cbmc">
    <sourcefiles>
      <includesfile>test/test-sets/VeriSec/Verisec_cbmc_bmc_pA_eA-set.set</includesfile>
    </sourcefiles>
    <option name="-cbmc"/>
  </test>
  
  <test name="-explicitAnalysis">
    <sourcefiles>
      <includesfile>test/test-sets/VeriSec/Verisec_cbmc_bmc_pA_eA-set.set</includesfile>
    </sourcefiles>
    <option name="-explicitAnalysis"/>
  </test>
  
<test name="-predicateAnalysis">
    <sourcefiles>
      <includesfile>test/test-sets/VeriSec/Verisec_cbmc_bmc_pA_eA-set.set</includesfile>
    </sourcefiles>
    <option name="-predicateAnalysis"/>
  </test>
  

  <columns>
    <column title="total">time for CPAchecker</column>
    <column title="reached">reached set</column>
    <column title="memory">Heap memory usage</column>
  </columns>
</benchmark>
