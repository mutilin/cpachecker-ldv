<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker" timelimit="900" memlimit="14000">
  <test name="combinations-a">
    <sourcefiles>
      <includesfile>test/assumptions/assumptions.set</includesfile>
    </sourcefiles>
    <option name="-config">test/config/restartAlgorithm-comb-a.properties</option>
    <option name="-setprop">output.disable=true</option>
    <option name="-heap">4000m</option>
  </test>
  <test name="combinations-b">
    <sourcefiles>
      <includesfile>test/assumptions/assumptions.set</includesfile>
    </sourcefiles>
    <option name="-config">test/config/restartAlgorithm-comb-b.properties</option>
    <option name="-setprop">output.disable=true</option>
    <option name="-heap">4000m</option>
  </test>
  <test name="combinations-c">
    <sourcefiles>
      <includesfile>test/assumptions/assumptions.set</includesfile>
    </sourcefiles>
    <option name="-config">test/config/restartAlgorithm-comb-c.properties</option>
    <option name="-setprop">output.disable=true</option>
    <option name="-heap">4000m</option>
  </test>
  <columns>
    <column title="total">time for CPAchecker</column>
  </columns>
</benchmark>