<?xml version="1.0" ?>

<table>

<!-- a list of testfiles, where each file was created automatically by benchmark.py for a test -->

  <test filename="test/results/benchmark-cpachecker.11-??-??.????.results.xml">
    <!-- columns "status" and "cputime" of the testfile will be part of the table -->
    <column title="status"/>
    <column title="cputime"/>
  </test>

  <test filename="test/results/benchmark-cpachecker.11-??-??.????.results.xml">
    <!-- all columns of the testfile will be part of the table -->
  </test>

  <union>
    <!-- Merge the result of several runs into one column in the table. -->
    <test filename="test/results/benchmark-cpachecker-part1.11-??-??.????.results.xml"/>
    <test filename="test/results/benchmark-cpachecker-part2.11-??-??.????.results.xml"/>
    <column title="status"/>
    <column title="cputime"/>
  </union>

</table>
