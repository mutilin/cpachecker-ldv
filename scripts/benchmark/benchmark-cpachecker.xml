<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpachecker">
  <test>
    <sourcefiles>$PATH_TO_CPACHECKER/test/programs/*/*.c</sourcefiles>
    <options>
      <option name="-setprop">output.disable=true</option>
      <option name="-config">$PATH_TO_CPACHECKER/test/config/alwaysTopAnalysis.properties</option>
    </options>
  </test>
  <test>
    <sourcefiles>$PATH_TO_CPACHECKER/test/programs/*/*.c</sourcefiles>
    <options>
      <option name="-setprop">output.disable=true</option>
      <option name="-config">$PATH_TO_CPACHECKER/test/config/concreteAnalysis.properties</option>
    </options>
  </test>
</benchmark>
